{
  "name": "handsfree",
  "version": "1.4.4",
  "description": "Add client side, multi-user, head-tracked-from-afar mouse cursors and handsfree user interfaces to your projects just...like...âœ¨...that!",
  "main": "dist/handsfree.js",
  "scripts": {
    "dev": "concurrently \"npm run lib:dev\" \"npm run docs:dev\"",
    "build": "concurrently \"npm run lib:build\" \"npm run docs:build\"",
    "lib:dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --mode development --config webpack.config.dev.js",
    "lib:build": "webpack -p --progress --mode production --config webpack.config.build.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "node deploy-docs.js",
    "test": "jest --detectOpenHandles"
  },
  "files": [
    "/dist/**/*",
    "/src/**/*"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "testURL": "http://localhost",
    "setupFiles": [
      "./mock/jest-canvas.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "mock/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "mock/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/handsdfreejs/handsfree.git"
  },
  "author": "Oz Ramos <labofoz@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/handsdfreejs/handsfree/issues"
  },
  "homepage": "https://github.com/handsdfreejs/handsfree#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.1",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "idempotent-babel-polyfill": "^6.26.0-1",
    "ify-loader": "^1.1.0",
    "jest-canvas-mock": "^1.1.0",
    "jest-cli": "^23.5.0",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "shelljs": "^0.8.2",
    "spectre.css": "^0.5.3",
    "style-loader": "^0.22.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vuepress": "^0.14.2",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@tensorflow-models/posenet": "^0.2.2",
    "@tensorflow/tfjs": "^0.12.5",
    "custom-event-polyfill": "^1.0.6",
    "dat.gui": "^0.7.2",
    "lodash": "^4.17.10",
    "stats.js": "^0.17.0"
  }
}
